<% if session[:linked_from] == "workout_index" %>
  <% @workout_names.each_with_index do |w_name, i| %>
  var sched_cal = document.getElementById("<%= w_name.split.join.downcase + '_cal' %>");
    sched_cal.innerHTML = "<%= escape_javascript(render partial: 'shared/calendar/schedule_calendar', locals: { cal_days: @days_display, wk_url: wk_find_url(w_name) }) %>";
  <% end %>
<% end %>

