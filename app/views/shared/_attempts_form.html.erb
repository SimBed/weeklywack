<%= form_with model: @attempt, local: true do |form| %>
  <%= render 'shared/error_messages', object: form.object %>
  <div class="field">
    <%= form.label :doa, 'Date of Attempt' %>
    <%= form.date_field :doa %>
  </div>
  <div class="field">
    <%= form.text_area :summary, placeholder: "Add your summary..." %>
  </div>
  <%= form.submit "Wack It", class: "btn btn-primary btnpost" %>
<% end %>
